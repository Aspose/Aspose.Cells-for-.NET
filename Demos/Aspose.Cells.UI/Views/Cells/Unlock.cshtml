@model ViewModel

@{
    Layout = "~/Views/Shared/Cells/Layout.cshtml";
}
@{
    ViewBag.Title = "Unlock";
}
@Html.Partial("Cells/Main")
@Html.Partial("Cells/FooterProduct")

@section scripts {

    <script type="text/javascript">
        var o = @Html.Raw(Model.JSOptions);
        $.extend(o, {
            Method: requestUnlock, 
            validationMessage: "@Html.Raw(Model.Resources["PasswordPlaceholderErrorMessage"])"
        });
    </script>
}