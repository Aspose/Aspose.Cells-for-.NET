@model ViewModel

@{
    Layout = "~/Views/Shared/Cells/Layout.cshtml";
}
@{
    ViewBag.Title = "Search";
}
@Html.Partial("Cells/Main")
@Html.Partial("Cells/FooterProduct", Model)

@section scripts {
    <script type="text/javascript">
        var o = @Html.Raw(Model.JSOptions);
        $.extend(o, {
            Method: requestSearch,
            validationSearchMessage: "@Html.Raw(Model.Resources["SearchQueryErrorMessage"])",
            FileAmountMessage:'@Model.Resources["FileAmountMessageOne"]',
            MaximumUploadFiles:1,
            Multiple: false
        });
    </script>
}