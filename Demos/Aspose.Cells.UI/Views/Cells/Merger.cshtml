@model ViewModel

@{
    Layout = "~/Views/Shared/Cells/Layout.cshtml";
}
@{
    ViewBag.Title = "Merger";
}
@Html.Partial("Cells/Main")
@Html.Partial("Cells/FooterProduct", Model)

@section scripts {

    <script type="text/javascript">
        var o = @Html.Raw(Model.JSOptions);
        $.extend(o, {
            Method: requestMerger,
            FileAmountMessage: '@Model.Resources["cellsMergerFileAmountMessage"]',
        });
    </script>
}