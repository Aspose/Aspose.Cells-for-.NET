@model ViewModel

@{
    Layout = "~/Views/Shared/Cells/Layout.cshtml";
}
@{
    ViewBag.Title = "Comparison";
}
@Html.Partial("Cells/Main")
@Html.Partial("Cells/FooterProduct")

@section scripts {

    <script type="text/javascript">
        var o = @Html.Raw(Model.JSOptions);
        $.extend(o, {
            Method: requestComparison,
            FileAmountMessage: '@Model.Resources["CompareFileAmountMessage"]',
            MaximumUploadFiles: 2
        });
    </script>
}