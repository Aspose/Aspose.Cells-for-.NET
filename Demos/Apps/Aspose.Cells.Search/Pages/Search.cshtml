@model Aspose.Cells.Common.Models.ViewModel

@{
    Layout = "~/Pages/Shared/Layout.cshtml";
}
@{
    ViewBag.Title = "Search";
}

@await Html.PartialAsync("Main")
@await Html.PartialAsync("FooterProduct")

@section scripts {
    <script type="text/javascript">
        const o = @Html.Raw(Model.JSOptions);
        $.extend(o, {
            Method: requestSearch,
            validationSearchMessage: "@Html.Raw(Model.Resources["SearchQueryErrorMessage"])",
            FileAmountMessage: '@Model.Resources["FileAmountMessageOne"]',
            MaximumUploadFiles: 1,
            Multiple: false
        });
    </script>
}