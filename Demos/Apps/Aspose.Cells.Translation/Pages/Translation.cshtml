@using Aspose.Cells.Common.Models
@model ViewModel

@{
    Layout = "~/Pages/Shared/Layout.cshtml";
}
@{
    ViewBag.Title = "Translation";
}

@await Html.PartialAsync("Main")
@await Html.PartialAsync("FooterProduct")

@section scripts {
    <script type="text/javascript">
        const o = @Html.Raw(Model.JSOptions);
        $.extend(o, {
            Method: requestTranslation,
            FileAmountMessage: '@Model.Resources["FileAmountMessageOne"]',
            MaximumUploadFiles: 1,
            Multiple: false
        });
    </script>
}