@model Aspose.Cells.Common.Models.ViewModel

@{
    Layout = "~/Pages/Shared/Layout.cshtml";
}
@{
    ViewBag.Title = "Metadata";
}

@await Html.PartialAsync("Main")
@await Html.PartialAsync("FooterProduct")

<script src="~/cells/@Model.AppName.ToLower()/content/scripts/metadata.js"></script>

@section scripts {
    <script type="text/javascript">
        const o = @Html.Raw(Model.JSOptions);
        $.extend(o, {
            Method: requestMetadata
        });
    </script>
}